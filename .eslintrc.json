{
    "extends": [
        "@nuxtjs/eslint-config-typescript"
    ],
    "rules": {
        "no-console": "off",
        "no-prototype-builtins": "off",
        "vue/multi-word-component-names": "off",
        "no-async-promise-executor": "off",
        "space-before-function-paren": ["error", "never"]
    }
}